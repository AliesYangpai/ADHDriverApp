        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:background="@color/white"
            android:orientation="vertical"
            android:paddingLeft="10dp"
            android:paddingRight="10dp">


            <!--货运类型-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="90dp"
                    android:layout_height="45dp"
                    android:gravity="center_vertical"
                    android:text="货运类型"
                    android:textColor="@color/black_text"
                    android:textSize="15sp" />


                <TextView
                    android:id="@+id/tv_car_type"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="50dp"
                    android:layout_weight="1"
                    android:text="..."
                    android:textColor="@color/gray_color"
                    android:textSize="14sp" />


            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:background="@color/bg_all" />


            <!--用车时间-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="90dp"
                    android:layout_height="45dp"
                    android:gravity="center_vertical"
                    android:text="用车时间"
                    android:textColor="@color/black_text"
                    android:textSize="15sp" />


                <TextView
                    android:id="@+id/tv_car_use_time"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="50dp"
                    android:layout_weight="1"
                    android:text="..."
                    android:textColor="@color/gray_color"
                    android:textSize="15sp" />


            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:background="@color/bg_all" />
            <!--货物名称-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="90dp"
                    android:layout_height="45dp"
                    android:gravity="center_vertical"
                    android:text="货物名称"
                    android:textColor="@color/black_text"
                    android:textSize="15sp"

                    />


                <TextView
                    android:id="@+id/tv_goods_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="50dp"
                    android:layout_weight="1"
                    android:text="..."
                    android:textColor="@color/gray_color"
                    android:textSize="15sp"

                    />


            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:background="@color/bg_all" />

            <!--发货人-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="90dp"
                    android:layout_height="45dp"
                    android:gravity="center_vertical"
                    android:text="发货人"
                    android:textColor="@color/black_text"
                    android:textSize="15sp"

                    />


                <TextView
                    android:id="@+id/tv_shipper_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="50dp"
                    android:layout_weight="1"
                    android:text="..."
                    android:textColor="@color/gray_color"
                    android:textSize="15sp"

                    />


            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:background="@color/bg_all" />

            <!--发货地址-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="90dp"
                    android:layout_height="45dp"
                    android:gravity="center_vertical"
                    android:text="发货地址"
                    android:textColor="@color/black_text"
                    android:textSize="15sp" />


                <TextView
                    android:id="@+id/tv_shipper_address"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="50dp"
                    android:layout_weight="1"
                    android:text="..."
                    android:textColor="@color/gray_color"
                    android:textSize="15sp"


                    />


            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:background="@color/bg_all" />

            <!--收货人-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="90dp"
                    android:layout_height="45dp"
                    android:gravity="center_vertical"
                    android:text="收货人"
                    android:textColor="@color/black_text"
                    android:textSize="15sp" />


                <TextView
                    android:id="@+id/tv_receiver_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="50dp"
                    android:layout_weight="1"
                    android:text="..."
                    android:textColor="@color/gray_color"
                    android:textSize="15sp" />


            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:background="@color/bg_all" />


            <!--收货地址-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <TextView

                    android:layout_width="90dp"
                    android:layout_height="45dp"
                    android:gravity="center_vertical"
                    android:text="收货地址"
                    android:textColor="@color/black_text"
                    android:textSize="15sp" />


                <TextView
                    android:id="@+id/tv_receiver_address"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="50dp"
                    android:layout_weight="1"
                    android:text="..."
                    android:textColor="@color/gray_color"
                    android:textSize="15sp" />


            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:background="@color/bg_all" />

            <!--载重量-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <TextView

                    android:layout_width="90dp"
                    android:layout_height="45dp"
                    android:gravity="center_vertical"
                    android:text="货物重量"
                    android:textColor="@color/black_text"
                    android:textSize="15sp" />


                <TextView
                    android:id="@+id/tv_goods_weight"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="50dp"
                    android:layout_weight="1"
                    android:text="..."
                    android:textColor="@color/gray_color"
                    android:textSize="15sp" />


            </LinearLayout>




            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:background="@color/bg_all" />

            <!--货物体积-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <TextView

                    android:layout_width="90dp"
                    android:layout_height="45dp"
                    android:gravity="center_vertical"
                    android:text="货物体积"
                    android:textColor="@color/black_text"
                    android:textSize="15sp" />


                <TextView
                    android:id="@+id/tv_goods_volume"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="50dp"
                    android:layout_weight="1"
                    android:text="..."
                    android:textColor="@color/gray_color"
                    android:textSize="15sp" />


            </LinearLayout>



            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:background="@color/bg_all" />

            <!--可得收益,注意该选项在竞价时候需要被隐藏-->
            <LinearLayout
                android:id="@+id/ll_freight"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="90dp"
                    android:layout_height="45dp"
                    android:gravity="center_vertical"
                    android:text="可得收益"
                    android:textColor="@color/black_text"
                    android:textSize="15sp" />


                <TextView
                    android:id="@+id/tv_freight"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="50dp"
                    android:layout_weight="1"
                    android:text="..."
                    android:textColor="@color/orange"
                    android:textSize="15sp"
                    android:textStyle="bold"

                    />


            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:background="@color/bg_all" />


            <!--给司机捎句话-->


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="90dp"
                    android:layout_height="45dp"
                    android:gravity="center_vertical"
                    android:text="备注"
                    android:textColor="@color/black_text"
                    android:textSize="15sp" />


                <TextView
                    android:id="@+id/tv_remark"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="50dp"
                    android:layout_weight="1"
                    android:text="..."
                    android:textColor="@color/gray_color"
                    android:textSize="15sp"

                    />


            </LinearLayout>


        </LinearLayout>


    </LinearLayout>































