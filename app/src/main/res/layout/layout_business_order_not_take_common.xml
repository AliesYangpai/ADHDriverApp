                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="10dp"
                    android:layout_weight="1"
                    android:text="..."
                    android:textColor="@color/orange"
                    android:textSize="15sp"
                    android:textStyle="bold"

                    />


            </LinearLayout>

        </LinearLayout>



