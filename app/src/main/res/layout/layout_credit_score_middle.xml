<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:Star="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"

    >




    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:gravity="center"
        >

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/synthesize_score"
            android:gravity="center"
            android:textSize="17sp"
            android:textColor="@color/white"
            />


        <com.adhdriver.work.ui.widget.rattingstar.StarRattingBar
            android:id="@+id/srb_synthesize_score"
            Star:starBackground="@drawable/img_star_empty"
            Star:starClickable="false"
            Star:starDistance="5dp"
            Star:starDrawBackground="@drawable/img_star_full"
            Star:starHeight="20dp"
            Star:starWidth="20dp"
            Star:starsNum="5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginTop="15dp"

            />




        

    </LinearLayout>

</LinearLayout>